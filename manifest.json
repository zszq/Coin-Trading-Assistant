{
  "manifest_version": 3,
  "name": "trading helper",
  "version": "1.0",
  "description": "coin trading helper extension",
  "permissions": ["storage", "declarativeContent", "activeTab", "scripting"],
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./images/icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/gate.js"],
      "matches": ["https://www.gate.io/*"],
      "run_at": "document_end"
    }
  ]
}
